{
    "name": "xiaohuilam/laravel-admin-nocaptcha",
    "description": "nocaptcha implements for laravel-admin when login",
    "type": "library",
    "require": {
        "encore/laravel-admin": "~1.6",
        "233sec/laravel-recaptchav3": "~0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "<8.0",
        "laravel/laravel": ">=5.5",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "<=v4.2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Xiaohui Lam",
            "email": "xiaohui.lam@aliyun.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaravelAdminExt\\Nocaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAdminExt\\Nocaptcha\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAdminExt\\Nocaptcha\\NocaptchaServiceProvider"
            ]
        }
    }
}
